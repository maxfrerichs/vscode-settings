{
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Dracula",
    "window.zoomLevel": 0.6,
    "editor.fontFamily": "'Source Code Pro', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontWeight": "bold",
    "terminal.integrated.fontWeight": "bold",
    "terminal.integrated.fontFamily": "MesloLGM Nerd Font",
    "workbench.colorCustomizations": {
        "sideBar.background": "#101011",
        "editor.background": "#101011",
        "terminal.background": "#101011",
        "terminal.foreground": "#e2e2e2",
        "panel.background": "#101011",
        "activityBar.background": "#101011",
        "tab.activeBorderTop": "#aeb2ff",
        "tab.inactiveBackground": "#191A22",
        "panelTitle.activeBorder": "#8d92ff",
        "panel.border": "#8d92ff",
        "tab.activeBackground": "#101011",
        "tab.inactiveForeground": "#868686",
        "breadcrumb.background": "#101011",
        "breadcrumb.foreground": "#ffffff",
        "sideBarSectionHeader.background": "#101011",
        "activityBarBadge.background": "#03cf36",
        "activityBar.activeBorder": "#03cf36",
        "titleBar.activeBackground": "#101011",
        "terminal.ansiGreen": "#20e3b2",
        "terminal.ansiYellow": "#FDE181",
        "terminal.ansiMagenta": "#ff6bcb",
        "editorCursor.foreground": "#9a86fd",
    },
    "editor.tokenColorCustomizations": {
        "[Dracula]": {
            "textMateRules": [
                {
                    "scope": [
                        "entity.other.attribute-name",
                        "entity.name.function",
                        "support.function",
                        "support.function.css",
                        "support.function.transform.css",
                        "entity.other.attribute-name.html",
                        "support.type.media-names.css",
                        "support.type.property-names.css",
                        "entity.other.attribute-name.jsx"
                    ],
                    "settings": {
                        "foreground": "#20e3b2",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other.property.js",
                        "variable.other.class.js",
                        "variable.other.property.static.js",
                        "JSXNested",
                        "variable.other.readwrite.js",
                        "punctuation.definition.tag.jsx",
                        "punctuation.semi-colon",
                        "punctuation.colon"
                    ],
                    "settings": {
                        "foreground": "#fff"
                    }
                },
                {
                    "scope": [
                        "variable.graphql",
                        "string",
                        "punctuation.definition.string.begin",
                        "punctuation.definition.string.end",
                        "meta.attribute-selector.scss",
                        "meta.attribute-selector.css"
                    ],
                    "settings": {
                        "foreground": "#EAC394"
                    }
                },
                {
                    "scope": [
                        "entity.name.tag",
                        "keyword",
                        "storage",
                        "constant.length.units.css",
                        "source.inside-js.css.styled",
                        "keyword.other.unit.percentage.css",
                        "keyword.other.unit.px.css",
                        "keyword.other.unit.rem.css",
                        "constant.length.units.css",
                        "constant.percentage.units.css",
                        "constant.time.units.css",
                        "constant.angle.units.css",
                        "constant.numeric.css",
                        "punctuation.definition.keyword"
                    ],
                    "settings": {
                        "foreground": "#ff6bcb"
                    }
                },
                {
                    "scope": [
                        "variable.parameter",
                        "variable.other.constant",
                        "support.variable",
                        "constant",
                        "support.type.property-name.media.css",
                        "meta.property-values.css",
                        "support.constant",
                        "variable.other.constant"
                    ],
                    "settings": {
                        "foreground": "#9a86fd",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "support.class.component"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other.constant.object.js"
                    ],
                    "settings": {
                        "foreground": "#bd93f9"
                    }
                },
                {
                    "scope": [
                        "entity.name.type.parameter"
                    ],
                    "settings": {
                        "foreground": "#ffb86c"
                    }
                },
                {
                    "scope": [
                        "support",
                        "entity.name.type",
                        "meta.interface.ts",
                        "entity.other.inherited-class"
                    ],
                    "settings": {
                        "foreground": "#2cccff",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other.readwrite.js",
                        "entity.name.type.class",
                        "variable.other.property.js",
                        "variable.scss"
                    ],
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#9dbeff"
                    }
                }
            ]
        }
    },
    "editor.fontSize": 16,
    "git.confirmSync": false,
    "gitlens.hovers.currentLine.over": "line",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.project.importOnFirstTimeStartup": "automatic",
    "launch": {

        "configurations": [],
        "compounds": []
    },
    "editor.minimap.enabled": false,
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "gitlens.codeLens.scopes": [
            "document"
        ]
    },
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.defaultProfile.linux": "zsh"
}
